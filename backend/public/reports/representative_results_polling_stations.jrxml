	<queryString>
		<![CDATA[(SELECT tp.id, tp.enrollment_id, tp.candidacy_id, 
	tp.number, tp.image, tp.`type`,
	tc.candidacy_name,
	CONCAT(RTRIM(ti.apellido1),' ',RTRIM(ti.apellido2),' ',RTRIM(ti.nombre1),' ',
	RTRIM(ti.nombre2)) AS names, tb.table_name, tb.table_number, COUNT(ta.id) AS total
	FROM tp_candidates AS tp
	LEFT JOIN student_enrollment AS tm ON tp.enrollment_id = tm.id 
	LEFT JOIN inscripciones AS ti ON tm.id_student   = ti.id 
	LEFT JOIN tp_candidacies AS tc ON tp.candidacy_id = tc.id 
	LEFT JOIN grados AS tg ON tm.id_grade = tg.id
	LEFT JOIN tp_aux_candidate_votes AS ta ON ta.candidate_id = tp.id
	LEFT JOIN tp_polling_stations AS tb ON ta.polling_station_id = tb.id
	WHERE tp.year =  $P{P_YEAR}   AND  tm.id_state = 2 AND tm.year =  $P{P_YEAR} AND 
	ta.year = $P{P_YEAR} AND tb.year = $P{P_YEAR}
	GROUP BY ta.candidate_id, ta.polling_station_id, ta.candidacy_id
	ORDER BY tc.candidacy_name, total DESC )
UNION 
(SELECT tp.id, 0 as enrollment_id, tp.candidacy_id, 
	'00' as number, tp.image, tp.`type`,
	tc.candidacy_name, tp.names, tb.table_name, tb.table_number, 
	COUNT(ta.id) AS total
	FROM tp_white_vote AS tp
	LEFT JOIN tp_candidacies AS tc ON tp.candidacy_id = tc.id 
	LEFT JOIN tp_aux_white_vote AS ta ON ta.white_vote_id = tp.id
	LEFT JOIN tp_polling_stations AS tb ON ta.polling_station_id = tb.id
	WHERE tp.year = $P{P_YEAR}  AND ta.year = $P{P_YEAR} AND tb.year = $P{P_YEAR}
	GROUP BY ta.white_vote_id, ta.polling_station_id, ta.candidacy_id
	ORDER BY tc.candidacy_name , total DESC)
	ORDER BY table_name]]>
	</queryString>
